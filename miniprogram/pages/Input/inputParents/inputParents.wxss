/* pages/Input/inputParents/inputParents.wxss */
.input-frame{
  display: flex;
  justify-content: center;
  border:1.5px solid #9ca582;
  width: 50%;
  height:10rpx;
  margin-top: 13rpx;
  margin-bottom: 20rpx;
}

.line{
  display: flex;
}

.text-wxss{
  display: flex;
  margin-top: 17rpx;
  justify-content: center;
  color: #9ca582;
}

/*radio.wx-radio-input.wx-radio-input-checked{
